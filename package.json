{
  "name": "quiz",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "dev:tailwind": "bunx tailwindcss -i ./src/tailwind.css -o ./build/index.css --watch",
    "dev:ts": "bun build --watch --outfile=./build/index.js ./src/index.ts",
    "dev:ts2": "bun build --watch --outfile=./build/login.js ./src/login.ts",
    "dev:ts3": "bun build --watch --outfile=./build/register.js ./src/register.ts",
    "dev:ts4": "bun build --watch --outfile=./build/aboutus.js ./src/aboutus.ts",
    "dev:ts5": "bun build --watch --outfile=./build/cart.js ./src/cart.ts",
    "dev:ts6": "bun build --watch --outfile=./build/info.js ./src/info.ts",
    "dev": "concurrently \"bun dev:tailwind\" \"bun dev:ts\" \"bun dev:ts2\" \"bun dev:ts3\" \"bun dev:ts4\" \"bun dev:ts5\" \"bun dev:ts6\""
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.0.3",
    "@types/bun": "latest",
    "concurrently": "^9.1.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "tailwindcss": "^4.0.3"
  }
}
